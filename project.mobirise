{
  "settings": {
    "name": "Liga Podwórkowa",
    "currentPage": "index.html",
    "ai": {
      "prompt": "create a football website about football competitions (league and cup). the website will contain an archive of results, match schedule, match photos, upcoming matches, all news and information about each team and general information about the league. there should be several subpages from the main page",
      "imageSearchQuery": "football"
    },
    "theme": {
      "name": "startm5",
      "title": "StartM5",
      "styling": {
        "backgroundColor": "#FFEEE6",
        "primaryColor": "#3A86FF",
        "secondaryColor": "#F8C04B",
        "successColor": "#2ED889",
        "infoColor": "#14CDF2",
        "warningColor": "#FFC107",
        "dangerColor": "#DA2C3C",
        "mainFont": "Source Serif 4",
        "display1Font": "Source Serif 4",
        "display1Size": 5,
        "display2Font": "Source Serif 4",
        "display2Size": 4,
        "display5Font": "Source Serif 4",
        "display5Size": 2.5,
        "display7Font": "Source Serif 4",
        "display7Size": "1",
        "display4Font": "Source Serif 4",
        "display4Size": "1",
